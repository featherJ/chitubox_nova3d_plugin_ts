{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceRoot}/out/main.js",
			"args": ["${workspaceRoot}/debug/source"],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"sourceMaps": true,
		}
	]
}